{
  "name": "kin-programming-language",
  "displayName": "Kin Programming Language",
  "description": "VS Code support for Kin Programming Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "kinlang",
      "aliases": ["Kin Programming Language", "kinlang"],
      "extensions": [".kin"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "kinlang",
      "scopeName": "source.kin",
      "path": "./syntaxes/kinlang.tmLanguage.json"
    }]
  }
}
